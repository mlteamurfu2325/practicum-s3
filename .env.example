# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=postgres
DB_USER=postgres
DB_PASSWORD=changeme  # Used by the application
POSTGRES_PASSWORD=changeme  # Must match DB_PASSWORD, used by Docker

# OpenRouter Configuration
OPENROUTER_API_KEY=your_api_key_here

# App Settings
MAX_RETRIES=3
TIMEOUT_SECONDS=15

# Security Settings
MAX_REQUESTS_PER_HOUR=100  # Rate limiting
